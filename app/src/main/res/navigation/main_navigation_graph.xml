<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph"
    app:startDestination="@id/fragmentMain">

    <fragment
        android:id="@+id/fragmentMain"
        android:name="com.beautymnl.exam.main.fragments.MainFragment"
        android:label="main"
        tools:layout="@layout/fragment_main">

        <action
            android:id="@+id/mainToContributorDetails"
            app:destination="@id/fragmentContributorDetails" />

    </fragment>

    <fragment
        android:id="@+id/fragmentContributorDetails"
        android:name="com.beautymnl.exam.details.ContributorDetailsFragment"
        android:label="contributorDetails"
        tools:layout="@layout/fragment_contributors_details" />

</navigation>